<zapaddon>
    <name>Reissue Request Scripter</name>
    <version>2</version>
    <description>Generate script to reproduce the HTTP request selected.</description>
    <author>Philippe Arteau (h3xStream)</author>
    <url>https://github.com/h3xstream/http-script-generator</url>
    <changes/>
    <dependson/>
    <extensions>
      <extension>org.zaproxy.zap.extension.scriptgen</extension>
    </extensions>
    <ascanrules/>
    <pscanrules/>
    <filters/>
    <files/>
    <not-before-version>2.2.2</not-before-version>
    <not-from-version/>
</zapaddon>
